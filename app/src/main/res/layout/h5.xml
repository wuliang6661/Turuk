<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">


    <com.haoyigou.hyg.ui.JocellWebView
        android:id="@+id/jocellWebView"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <RelativeLayout
        android:id="@+id/linear_setting"
        android:layout_width="match_parent"
        android:layout_height="68dp"
        android:background="@drawable/change_color"
        android:orientation="horizontal"
        android:visibility="visible">

        <LinearLayout
            android:id="@+id/setting_backto"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <ImageView
                android:layout_width="18dp"
                android:layout_height="18dp"
                android:focusable="true"
                android:layout_marginStart="12dp"
                android:layout_marginTop="30dp"
                android:focusableInTouchMode="true"
                android:src="@drawable/back_to_superior" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:singleLine="true"
                android:layout_marginTop="30dp"
                android:textColor="#fff"
                android:textSize="12dp"
                android:layout_marginLeft="2dp"
                android:text="首页"/>

        </LinearLayout>

        <TextView
            android:id="@+id/tvHearder"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="30dp"
            android:gravity="center"
            android:text="设置"
            android:textColor="@color/white"
            android:textSize="16sp" />

    </RelativeLayout>
</RelativeLayout>
